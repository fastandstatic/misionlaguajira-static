 > 10) {\r\n                valorActual = valorActual.substring(0, 10);\r\n            }\r\n            $(this).val(valorActual);\r\n        });\r\n\r\n        form.on('submit', function (e) {\r\n            e.preventDefault(); \/\/ Evita el env\u00edo del formulario por defecto\r\n\r\n            \/\/ Validar que el n\u00famero de tel\u00e9fono tenga 10 d\u00edgitos\r\n            const telefonoValor = telefonoInput.val();\r\n            if (telefonoValor.length !== 10) {\r\n                Swal.fire({\r\n                    title: \"Error\",\r\n                    text: \"El n\u00famero de tel\u00e9fono debe tener exactamente 10 d\u00edgitos.\",\r\n                    icon: \"error\",\r\n                    confirmButtonColor: \"#d33\",\r\n                    confirmButtonText: \"Aceptar\"\r\n                });\r\n                return; \/\/ Salir de la funci\u00f3n para no enviar el formulario\r\n            }\r\n\r\n            \/\/ Cerrar el popup de Elementor si est\u00e1 abierto\r\n            const elementorPopup = $('.elementor-popup-modal');\r\n            if (elementorPopup.length) {\r\n                elementorPopup.fadeOut();\r\n            }\r\n\r\n            \/\/ Captura los datos del formulario\r\n            const formData = form.serializeArray();\r\n            const data = {};\r\n            formData.forEach((input) => {\r\n                data[input.name] = input.value;\r\n            });\r\n\r\n            \/\/ Agregar UTM parameters\r\n            const urlParams = new URLSearchParams(window.location.search);\r\n            const utm_source = urlParams.get('utm_source');\r\n            const utm_medium = urlParams.get('utm_medium');\r\n            const utm_campaign = urlParams.get('utm_campaign');\r\n            const utm_term = urlParams.get('utm_term');\r\n            const utm_content = urlParams.get('utm_content');\r\n\r\n            data['utm_source'] = utm_source;\r\n            data['utm_medium'] = utm_medium;\r\n            data['utm_campaign'] = utm_campaign;\r\n            data['utm_term'] = utm_term;\r\n            data['utm_content'] = utm_content;\r\n\r\n            \/\/ Muestra la alerta con la informaci\u00f3n del usuario\r\n            Swal.fire({\r\n                title: \"\u00a1Tu mensaje fue recibido!\",\r\n                html: `\r\n                    <div style=\"text-align: left;\">\r\n                        <p style=\"margin-bottom: 10px;\"><strong>Gracias por contactarnos.<\/strong><\/p>\r\n                        <ul style=\"list-style-type: none; padding-left: 0;\">\r\n                            <li><strong>Nombre Completo:<\/strong> ${data['form_fields[chat_name]']}<\/li>\r\n                            <li><strong>WhatsApp:<\/strong> ${data['form_fields[chat_phone]']}<\/li>\r\n                            <li><strong>Email:<\/strong> ${data['form_fields[chat_email]']}<\/li>\r\n                            <li><strong>Industria de la empresa:<\/strong> ${data['form_fields[interesado]']}<\/li>\r\n                            <li><strong>Comentarios:<\/strong> ${data['form_fields[message]']}<\/li>\r\n                        <\/ul>\r\n                    <\/div>\r\n                `,\r\n                icon: \"success\",\r\n                confirmButtonColor: \"#EF8A4E\",\r\n                confirmButtonText: \"Continuar\",\r\n                customClass: {\r\n                    container: 'swal-container',\r\n                    title: 'swal-title',\r\n                    content: 'swal-content',\r\n                    confirmButton: 'swal-confirm-button',\r\n                },\r\n                customContainerClass: 'swal-custom-container',\r\n                customCSS: {\r\n                    background: '#fff',\r\n                    border: '2px solid #ffff',\r\n                    borderRadius: '8px',\r\n                    color: '#EF8A4E',\r\n                    fontFamily: 'Arial, sans-serif',\r\n                },\r\n            }).then((result) => {\r\n                \/\/ Ocultar el popup de Elementor Pro despu\u00e9s de que se cierre la alerta\r\n                if (window.elementorProFrontend && window.elementorProFrontend.modules.popup) {\r\n                    const popup = window.elementorProFrontend.modules.popup.getPopupView();\r\n                    if (popup) {\r\n                        popup.hide();\r\n                    }\r\n                }\r\n            });\r\n\r\n            \/\/ Env\u00eda los datos del formulario por webhook\r\n            fetch('https:\/\/hook.us2.make.com\/i1jm9ghj498tu6b5j2hl9w1xdr8lnv8w', {\r\n                method: 'POST',\r\n                body: JSON.stringify(data),\r\n                headers: {\r\n                    'Content-Type': 'application\/json'\r\n                }\r\n            })\r\n            .then(response => {\r\n                if (!response.ok) {\r\n                    throw new Error('Error al enviar los datos por el webhook');\r\n                }\r\n                return response.json();\r\n            })\r\n            .then(responseData => {\r\n                console.log('Respuesta del webhook:', responseData);\r\n\r\n                \/\/ Enviar evento a Facebook Pixel con el ID espec\u00edfico\r\n                fbq('track', 'Lead', {\r\n                    content_name: 'Formulario Enviado',\r\n                    value: 1.00,\r\n                    currency: 'USD',\r\n                    eventID: '868007421429361' \/\/ Facebook Pixel ID\r\n                });\r\n\r\n                \/\/ Enviar evento a Google Analytics 4 con el ID espec\u00edfico\r\n                gtag('event', 'Formulario Enviado', {\r\n                    'send_to': 'G-L4E35LS899', \/\/ GA4 Measurement ID\r\n                    'event_category': 'Formulario',\r\n                    'event_label': 'Chat_whatsapp_enviado',\r\n                    'value': 1.00\r\n                });\r\n\r\n            })\r\n            .catch(error => {\r\n                console.error('Error:', error);\r\n            });\r\n\r\n            \/\/ Enviar evento a Google Analytics Universal cuando se env\u00eda el formulario de WhatsApp\r\n            gtag('event', 'Chat_whatsapp_enviado', {\r\n                'event_category': 'Formulario',\r\n                'event_label': 'Chat_whatsapp_enviado',\r\n            });\r\n\r\n            \/\/ Enviar evento al p\u00edxel de Facebook cuando se env\u00eda el formulario de WhatsApp\r\n            fbq('track', 'Chat_whatsapp_enviado', {\r\n                'event_category': 'Formulario',\r\n                'event_label': 'Chat_whatsapp_enviado',\r\n            });\r\n        });\r\n    }\r\n})(jQuery);\r\n\r\n<\/script>\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t<div class=\"elementor-element elementor-element-c000d59 e-con-full elementor-hidden-laptop elementor-hidden-tablet elementor-hidden-mobile e-flex e-con e-child\" data-id=\"c000d59\" data-element_type=\"container\">\n\t\t\t\t<div class=\"elementor-element elementor-element-322a89c elementor-widget elementor-widget-image\" data-id=\"322a89c\" data-element_type=\"widget\" data-widget_type=\"image.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t\t\t\t\t\t\t\t\t<img fetchpriority=\"high\" decoding=\"async\" width=\"824\" height=\"180\" src=\"https:\/\/misionlaguajira.fastandstatic.com\/wp-content\/uploads\/2024\/05\/Grupo-14538.png\" class=\"attachment-full size-full wp-image-2147\" alt=\"\" srcset=\"https:\/\/misionlaguajira.fastandstatic.com\/wp-content\/uploads\/2024\/05\/Grupo-14538.png 824w, https:\/\/misionlaguajira.fastandstatic.com\/wp-content\/uploads\/2024\/05\/Grupo-14538-300x66.png 300w, https:\/\/misionlaguajira.fastandstatic.com\/wp-content\/uploads\/2024\/05\/Grupo-14538-768x168.png 768w\" sizes=\"(max-width: 824px) 100vw, 824px\" \/>\t\t\t\t\t\t\t\t\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t","protecte